{
  "name": "@starter/web",
  "version": "1.0.0",
  "description": "Frontend for starter",
  "private": "true",
  "homepage": "https://zkindest.github.io/starter-fullstack-rest",
  "dependencies": {
    "@trpc/client": "9.27.2",
    "@trpc/react": "9.27.2",
    "@trpc/server": "9.27.2",
    "broadcast-channel": "4.17.0",
    "jwt-decode": "3.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.39.2",
    "react-router-dom": "6.4.1",
    "superjson": "1.10.0",
    "web-vitals": "2.1.4",
    "zustand": "3.7.2"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint  --quiet --fix --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore --write",
    "deploy": "bash ./deploy.sh",
    "msw:init": "msw init public/ --save"
